#! /bin/bash
# /etc/init.d/garage-pi

OUT_LOG=/dev/null
GOPATH=/go

case "$1" in
  start)
    echo "Starting garage-pi"
    $GOPATH/bin/garage-pi 1>$OUT_LOG 2>$OUT_LOG &
    ;;
  stop)
    echo "Stopping garage-pi"
    killall garage-pi
    ;;
  *)
    echo "Usage: /etc/init.d/garage-pi {start|stop}"
    exit 1
    ;;
  esac
exit 0
